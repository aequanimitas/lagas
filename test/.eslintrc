{
  "env": {
    "mocha": true
  },
  "globals": {
    "assert": true,
    "expect": true,
    "sinon": true
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-expressions": 0,
    "constructor-super": 2,
    "jsx-qoutes": 0,
    "no-eq-null": 0,
    "prefer-const": 0,
    "no-param-reassign": 0,
    "no-this-before-super": 2,
    "babel/object-shorthand": 2,
    "react/jsx-boolean-value": 0,
  }
}
